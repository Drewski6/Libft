    "%%file ft_strlen.c\n",
    "/* ************************************************************************** */\n",
    "/*                                                                            */\n",
    "/*                                                        :::      ::::::::   */\n",
    "/*   ft_strlen.c                                        :+:      :+:    :+:   */\n",
    "/*                                                    +:+ +:+         +:+     */\n",
    "/*   By: dpentlan <marvin@42.fr>                    +#+  +:+       +#+        */\n",
    "/*                                                +#+#+#+#+#+   +#+           */\n",
    "/*   Created: 2022/11/11 12:59:37 by dpentlan          #+#    #+#             */\n",
    "/*   Updated: 2022/11/11 12:59:45 by dpentlan         ###   ########.fr       */\n",
    "/*                                                                            */\n",
    "/* ************************************************************************** */\n",
    "\n",
    "#include \"libft.h\"\n",
    "\n",
    "long\tft_strlen(const char *str)\n",
    "{\n",
    "\tlong\ti;\n",
    "\n",
    "\ti = 0;\n",
    "\twhile (str[i] != '\\0')\n",
    "\t\ti++;\n",
    "\treturn (i);\n",
    "}\n"
   ]
