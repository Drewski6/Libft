    "\n",
    "#include \"libft.h\"\n",
    "\n",
    "int\tft_toupper(int c)\n",
    "{\n",
    "\tif (c >= 97 && c <= 122)\n",
    "\t\treturn (c - 32);\n",
    "\telse\n",
    "\t\treturn (c);\n",
    "}"
