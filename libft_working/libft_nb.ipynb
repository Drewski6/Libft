    "%%file ft_isalpha.c\n",
    "/* ************************************************************************** */\n",
    "/*                                                                            */\n",
    "/*                                                        :::      ::::::::   */\n",
    "/*   ft_isalpha.c                                       :+:      :+:    :+:   */\n",
    "/*                                                    +:+ +:+         +:+     */\n",
    "/*   By: dpentlan <marvin@42.fr>                    +#+  +:+       +#+        */\n",
    "/*                                                +#+#+#+#+#+   +#+           */\n",
    "/*   Created: 2022/11/07 15:11:16 by dpentlan          #+#    #+#             */\n",
    "/*   Updated: 2022/11/07 15:11:19 by dpentlan         ###   ########.fr       */\n",
    "/*                                                                            */\n",
    "/* ************************************************************************** */\n",
    "\n",
    "#include \"libft.h\"\n",
    "\n",
    "int\tft_isalpha(char c)\n",
    "{\n",
    "\tif ((c >= 65 && c <= 90) || (c >= 97 && c <= 122))\n",
    "\t\treturn (1);\n",
    "\telse\n",
    "\t\treturn (0);\n",
    "}\n"
   ]
