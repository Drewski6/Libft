    "/*                                                                            */\n",
    "/* ************************************************************************** */\n",
    "\n",
    "#include \"libft.h\"\n",
    "\n",
    "char\t*ft_strnstr(const char *big, const char *little, size_t len)\n",
    "{\n",
    "\tsize_t\ti;\n",
    "\tsize_t\tj;\n",
    "\n",
    "\ti = 0;\n",
    "\tif (*little == 0)\n",
    "\t\treturn ((char *)big);\n",
    "\twhile (big[i] && i < len)\n",
    "\t{\n",
    "\t\tj = 0;\n",
    "\t\twhile (big[i + j] == little[j] && little[j] != 0)\n",
    "\t\t{\n",
    "\t\t\tj++;\n",
    "\t\t}\n",
    "\t\tif (little[j] == 0 && i + j <= len)\n",
    "\t\t\treturn ((char *)big + i);\n",
    "\t\ti++;\n",
    "\t}\n",
    "\treturn (0);\n",
    "}"
   ]
